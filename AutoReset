local Players = game:GetService("Players")
local Lp = Players.LocalPlayer
local Bp = Lp.Backpack
local Character = Lp.Character or Lp.CharacterAdded:Wait()
local Humanoid = Character:FindFirstChild("Humanoid") or Character:WaitForChild("Humanoid")
local Count = 0
for i, v in pairs(Bp:GetChildren()) do
	if v:IsA("Tool") and table.find(getgenv().Moves, v.Name) then
		getgenv().Reset = false
	else
		Character:BreakJoints()
	end
end
for i,v in pairs(getgenv().Moves) do 
	print(i, v)
end
Lp.CharacterAdded:Connect(function(Char)
	Bp = Lp.Backpack
	Bp.ChildAdded:Connect(function(Child)
		Count = Count + 1
		print("Count Incrementing:", Count)
		if Child:IsA("Tool") and table.find(getgenv().Moves, Child.Name) and getgenv().Reset == true and Count == 4 then
			print("Count After If-Statement Is Ran:", Count)
			print("You got a move in the table!", Child)
			getgenv().Reset = false
		elseif getgenv().Reset == true and Count == 4 then
			for i,v in pairs(Bp:GetChildren()) do 
			if v:IsA("Tool") then
			print(v.Name)
			end 
		end
			print("Count Before Reset:", Count)
			Count = 0
			Char:BreakJoints()
			print("Count After Reset:", Count)
		end
	end)
end)
